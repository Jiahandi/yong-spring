<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xian.yong.mapper.AdminMapper">
    <update id="update">
        update `admin`
        <set>
            <if test="type != null">
                type = #{type},
            </if>
            <if test="typaname != null">
                typaname = #{typaname},
            </if>

        </set>
        <where>
            adid = #{adid}
        </where>

    </update>
</mapper>